{
  "name": "sequelize-paper-trail",
  "version": "0.2.0",
  "description": "Track changes to your Sequelize models data. Perfect for auditing or versioning.",
  "bugs": {
    "url": "https://github.com/nielsgl/sequelize-paper-trail/issues",
    "email": "nvangalenlast@gmail.com"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">=5.0 <6",
    "npm": ">=3.3 <4"
  },
  "scripts": {
    "test": "mocha test/* --reporter dot --compilers js:babel-register --require ./test/helper.js --recursive",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run lint && babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nielsgl/sequelize-trails.git"
  },
  "private": false,
  "keywords": [
    "sequelize",
    "sequelize-plugin",
    "audit",
    "paper trail",
    "audit trail",
    "trail",
    "revision",
    "revisions",
    "versioning",
    "nodejs"
  ],
  "author": {
    "name": "Niels van Galen Last",
    "email": "nvangalenlast@gmail.com",
    "url": "https://nielsgl.com"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-flow-strip-types",
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "app-module-path": "^1.0.6",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flow-vars": "^0.3.0",
    "eslint-plugin-flowtype": "^2.2.6",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "mversion": "^1.10.1",
    "pretty-error": "^2.0.0",
    "sqlite3": "^3.1.3"
  },
  "dependencies": {
    "deep-diff": "^0.3.3",
    "diff": "^2.2.2",
    "lodash": "^4.11.1",
    "sequelize": "^3.21.0"
  }
}
